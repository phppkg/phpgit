{
    "name": "phppkg/phpgit",
    "type": "library",
    "description": "A Git wrapper library for PHP 8.1+",
    "homepage": "https://github/phppkg/phpgit.git",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "inhere",
            "email": "in.798@qq.com"
        },
        {
            "name": "Kazuyuki Hayashi",
            "email": "hayashi@valnur.net"
        }
    ],
    "require": {
        "php": ">8.1.0",
        "toolkit/cli-utils":"~2.0",
        "toolkit/stdlib":"~2.0",
        "symfony/process":"~6.0 || ~7.0",
        "symfony/options-resolver": "~6.0 || ~7.0"
    },
    "require-dev": {
        "symfony/filesystem": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "PhpGit\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
